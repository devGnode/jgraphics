  "dependencies": {
    "@types/node": "^14.14.22",
    "@types/typescript": "^2.0.0",
    "lib-utils-ts": "^1.3.1-beta",
    "logger20js-ts": "^1.2.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
