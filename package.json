{
  "name": "web-canvas-core",
  "version": "1.0.0",
  "description": "Canvas wrapping class.",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build:test": "tsc --module amd --target es2020 --sourceMap true --outDir ./prod-web ./Test/ginit.ts",

    "amd:build:prod": "tsc --module amd --outDir ./prod-web --declaration true",
    "common:build": "tsc --module commonjs --outDir ./prod-common",

    "tools": "tsc --module amd --outDir ./prod-web/tools tools/packSprite.ts",
    "run:packSprite": "ts-node --project ./tools/tsconfig.json ./tools/packSprite.ts"
  },
